! Modificator key
keycode 62 = Mode_switch NoSymbol

! Arrow keys
keycode 45 = Cyrillic_el Cyrillic_EL Left Left
keycode 47 = Cyrillic_zhe Cyrillic_ZHE Right Right
keycode 32 = Cyrillic_shcha Cyrillic_SHCHA Up Up
keycode 46 = Cyrillic_de Cyrillic_DE Down Down

! BackSpace, Insert and Delete
keycode 44 = Cyrillic_o Cyrillic_O BackSpace BackSpace
keycode 31 = Cyrillic_sha Cyrillic_SHA Insert Insert
keycode 33 = Cyrillic_ze Cyrillic_ZE Delete Delete

